{{#each guilds}}
    <button type="button" class="collapsible main_btn">
        <div class="guild_button">
            <img src="{{this.icon_url}}" alt="{{this.name}} ({{this.id}}) icon" class="icon" >
        </div>
        <div class="guild_button">
            <h3>{{this.name}} ({{this.id}})</h3>
        </div>
    </button>
    <div class="content">
        <button type="button" class="not_collapsible">ID: {{this.id}}</button>
        <button type="button" class="not_collapsible">Name: {{this.name}}</button>
        <button type="button" class="collapsible">
            <div class="guild_button">
                <img src="{{this.owner.avatar_url}}" alt="{{this.owner.username}} ({{this.owner.id}}) avatar" class="avatar" >
            </div>
            <div class="guild_button">
                <h4>Owner: {{this.owner.username}}#{{this.owner.discriminator}}</h3>
            </div>
        </button>
        <div class="content">
            <button type="button" class="not_collapsible">ID: {{this.owner.id}}</button>
            <button type="button" class="not_collapsible">Username: {{this.owner.username}}</button>
            <button type="button" class="not_collapsible">Nickname: {{this.owner.nickname}}</button>
            <button type="button" class="not_collapsible">Discriminator: {{this.owner.discriminator}}</button>
        </div>
        <button type="button" class="not_collapsible">Members: {{this.members}}</button>
        <button type="button" class="not_collapsible">Bots: {{this.bots}}</button>
        <a href="/?guild_id={{this.id}}"><button type="button" class="not_collapsible">View guild</button></a>
    </div>
{{/each}}