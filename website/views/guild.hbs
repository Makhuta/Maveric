<button type="button" class="not_collapsible main_btn active">
    <div class="guild_button">
        <img src="{{guild_data.icon_url}}" alt="{{guild_data.name}} ({{guild_data.id}}) icon" class="icon" >
    </div>
    <div class="guild_button">
        <h3>{{guild_data.name}} ({{guild_data.id}})</h3>
    </div>
</button>
<div class="content not_colapsable">
    <button type="button" class="not_collapsible">ID: {{guild_data.id}}</button>
    <button type="button" class="not_collapsible">Name: {{guild_data.name}}</button>
    <button type="button" class="collapsible">
        <div class="guild_button">
            <img src="{{guild_data.owner.avatar_url}}" alt="{{guild_data.owner.username}} ({{guild_data.owner.id}}) avatar" class="avatar" >
        </div>
        <div class="guild_button">
            <h4>Owner: {{guild_data.owner.username}}#{{guild_data.owner.discriminator}}</h3>
        </div>
    </button>
    <div class="content">
        <button type="button" class="not_collapsible">ID: {{guild_data.owner.id}}</button>
        <button type="button" class="not_collapsible">Username: {{guild_data.owner.username}}</button>
        <button type="button" class="not_collapsible">Nickname: {{guild_data.owner.nickname}}</button>
        <button type="button" class="not_collapsible">Discriminator: {{guild_data.owner.discriminator}}</button>
    </div>
    <button type="button" class="collapsible">Members ({{guild_data.members.length}})</button>
        <div class="content">
            {{#each guild_data.members}}
                {{> user}}
            {{/each}}
        </div>
    <button type="button" class="collapsible">Bots ({{guild_data.bots.length}})</button>
        <div class="content">
            {{#each guild_data.bots}}
                {{> user}}
            {{/each}}
        </div>
    <a href="/"><button type="button" class="not_collapsible">Back</button></a>
</div>