    }

    .placeholder_child {
      border: 1px solid black;
    }

    .odkazy_format {
      margin: 5px;
      border-radius: inherit;
      font-size: 22px;
      font-family: "Verdana";
      color: #99AAB5;
    }

    .remove_decor {
      text-decoration: none;
    }
  </style>
</head>

