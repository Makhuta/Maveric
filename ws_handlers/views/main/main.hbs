<body onload="main_restart_time({{start_time}})">
  <div class="nadpis"> <b class="nadpis_text_format">🎮 🎮 🎮 NSBR 🎮 🎮 🎮</b> </div>
  <div class="placeholder_main">
    <div class="placeholder_father">
      <div class="slider_name">
        <b class="slider_name_text">NSBR Rozcestník</b>
      </div>
      <div class="slider">
        {{#each sites}}<a href="{{{this.site}}}" class="remove_decor">
          <div class="slide"><b class="odkazy_format">{{this.name}}</b></div>
        </a>
        {{/each}}
      </div>
    </div>
    <div class="placeholder_father" style="float: right;">
      <div class="slider_name">
        <b class="slider_name_text">Members</b>
      </div>
      <div class="slider">
        {{#each counter}}
        <div class="slide"><b class="odkazy_format">{{this}}</b></div>
        {{/each}}
      </div>
    </div>
    <div class="placeholder_father" style="float: right;">
      <div class="slider_name">
        <b class="slider_name_text">Momentální Aktivita</b>
      </div>
      <div class="slider">
        {{#each channel_list}}
        <div class="slide"><b class="odkazy_format">{{this.channel_name}}</b></div>
        {{#each this.users}}
        <div class="slide" style="margin-left: 5%; width: 94%;"><b class="odkazy_format" style="font-size: 16px;">-
            {{this.user_username}}</b><b class="odkazy_format" style="font-size: 12px; float: right;">
            {{this.user_activity}}</b></div>
        {{/each}}
        {{/each}}
      </div>
    </div>
  </div>
  <div class="counter_placeholder" style="left: 0;">
    <div class="counter"><b>Restart proběhl před: </b><b class="counter" id="time"
        style="background: none; border: none; text-align:left; width: fit-content; height: 24px">0:000:00:00:00</b>
    </div>
  </div>
  <div class="counter_placeholder" style="right: 0;">
    <div class="counter"><b>Od posledního restartu navštívilo: {{visitors}}</b></div>
  </div>
</body>